package assign2_3;

public class DateTest {

	public static void main(String[] args) {

		Date d1 = new Date();
		d1.displaytData();
		Date d2 = new Date(10,10,2023);
		d2.displaytData();
		System.out.println("Day : "+d2.getDay());
		System.out.println("Month : "+d2.getMonth());
		System.out.println("Year : "+d2.getYear());
	}

}
