package assign2_3;

public class Date {
	private int day;
	private int month;
	private int year;
	
	public Date() {
		this.day = 00;
		this.month = 00;
		this.year = 00;
	}

	public Date(int day, int month, int year) {
		this.day = day;
		this.month = month;
		this.year = year;
	}

	public int getDay() {
		return day;
	}

	public void setDay(int date) {
		this.day = date;
	}

	public int getMonth() {
		return month;
	}

	public void setMonth(int month) {
		this.month = month;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}
	
	@Override
	public String toString() {
		return "Date : " + day + "/" + month + "/" + year;
	}

	//public String displaytData() {
		//return " Day : "+this.day+"/"+this.month+"/"+this.year;
	//}
	
	
}
